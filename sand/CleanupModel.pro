pro CleanupModel	scenes=findfile('*')	cd, current=masterDIR	print, scenes		fileNames=['SandTransportMap', 'fullNDVI', 'classNDVItmp', 'classNDVItmp6', 'classNDVItmp11', $		'classNDVItmp16', 'classNDVItmp21', 'classNDVItmp26', 'classNDVItmp31', 'classNDVItmp36', $		'classNDVItmp41', 'FutureVegMap', 'FutureVegMapCorrected', 'NDVIvsCentMeta.out', $		'NDVIvsCentMeta .out', 'NDVIvsCentMeta11.out', 'NDVIvsCentMeta16.out', 'NDVIvsCentMeta21.out', $		'NDVIvsCentMeta26.out', 'NDVIvsCentMeta31.out', 'NDVIvsCentMeta36.out', 'NDVIvsCentMeta41.out']		for i=0, n_elements(scenes)-1 do begin		print, scenes(i)		current=strtok(scenes(i),':', /extract)		if (strtok(scenes(i), '.'))(0) eq 0 and file_test(current(0), /directory) then begin			cd, current(0)			print, ' ', string(byte(13))			print, '**************************',current(0),'**************************'						for j=0, n_elements(fileNames)-1 do begin				if file_test(fileNames(j)) then begin					file_delete, fileNames(j)				endif			endfor						cd, masterDIR		endif	endforend