pro readc3c4, fname	openr, un, /get, fname	s=''	j=0	out=fltarr(5,1)	readf, un, s	s=''	while not eof(un) do begin		s=''		readf, un, s		sep = strsplit(s, '	')		i=fltarr(5)		for k=0,4 do begin			i(k) = strmid(s, sep(k+2),sep(k+3))		endfor				out=[[out], [i]]		j=j+1	endwhile		print, out	print, j	close, un	outf=string(fname+'.out')	openw, un, outf	printf,un, out	free_lun, un	end