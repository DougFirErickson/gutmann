function isAnImage, fname	if file_test(fname) and not file_test(fname, /Directory) then begin		openr, un, /get, fname		size=(fstat(un)).size		close, un		free_lun, un		return, size gt 40000000	endif else return, 0 eq 1endpro makeallsandmasks	envistart		fileList = findfile('*')		for i=0, n_elements(fileList)-1 do begin		if isAnImage(fileList(i)) then begin			evfToMask, 'sand.evf', fileList(i), fileList(i)+'.evf'		endif	endforend