pro sitesch, outdir, sitefile,wthfile,inityear,crop,schfile, daycent=daycent	cd, outdir, current=current	openw, oun, schfile, /get        if not keyword_set(daycent) then begin           printf, oun, '0             Starting year'        endif else printf, oun, inityear+'           Starting year'        printf, oun, '2002          Last year'        printf, oun, sitefile,'               Site file name'        printf, oun, '0             Labeling type'        printf, oun, '-1            Labeling year'        printf, oun, '-1.00         Microcosm'        printf, oun, '-1             CO2 Systems'        printf, oun, '1             Initial system'        printf, oun, crop,'           Initial crop'        printf, oun, '              Initial tree'        printf, oun, ' '        printf, oun, 'Year Month Option'        if not keyword_set(daycent) then begin           printf, oun, '1             Block #'           printf, oun, strcompress(inityear,/remove_all),'          Last year'           printf, oun, '1             Repeats # years'           printf, oun, strcompress(inityear-1,/remove_all),'          Output starting year'           printf, oun, '1             Output month'           printf, oun, '0.083         Output interval'           printf, oun, 'S             Weather choice'           printf, oun, '   1    1 CROP'           printf, oun, crop           printf, oun, '   1    1 FRST'           printf, oun, '   1   11 SENM'           printf, oun, '   1   12 LAST'           printf, oun, '-999 -999 X'           printf, oun, '2             Block #'        endif else printf, oun, '1             Block #'	printf, oun, '2002          Last year'	printf, oun, '1             Repeats # years'	printf, oun, strcompress(inityear+1,/remove_all),'          Output starting year'	printf, oun, '1             Output month'	printf, oun, '0.083         Output interval'	printf, oun, 'F             Weather choice'	printf, oun, wthfile	printf, oun, '   1    1 CROP'	printf, oun, crop	printf, oun, '   1    1 FRST'	printf, oun, '   1   11 SENM'	printf, oun, '   1   12 LAST'	printf, oun, '-999 -999 X'		close, oun		free_lun, oun	cd, currentend