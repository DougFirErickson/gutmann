pro combndvi, in1, in2, in3, in4, in5, in6, in7, in8, in9, in10, in11, outns=1192nl=708nb=2nyrs=11openr, un1, /get, in1openr, un2, /get, in2openr, un3, /get, in3openr, un4, /get, in4openr, un5, /get, in5openr, un6, /get, in6openr, un7, /get, in7openr, un8, /get, in8openr, un9, /get, in9openr, un10, /get, in10openr, un11, /get, in11unitarray=[un1, un2, un3, un4, un5, un6, un7, un8, un9, un10, un11]openw, oun, /get, outdata=bytarr(ns,nl, nb)ndvi=intarr(ns,nl)for i=0, nyrs-1 do begin	readu, unitarray(i), data	ndvi=fix(((float(data(*,*,1)) - float(data(*,*,0))) / $		  (float(data(*,*,1)) + float(data(*,*,0)))     )*10000)	writeu, oun, ndvi	close, unitarray(i)	free_lun, unitarray(i)endforclose, ounfree_lun, ounend